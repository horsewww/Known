@using Known.Razor
@using Microsoft.AspNetCore.Components.Routing
@inherits BaseComponent

<CascadingValue Value="Context">
    <Router AppAssembly="typeof(App).Assembly">
        <Found Context="routeData">
            <RouteView RouteData="@routeData" />
        </Found>
        <NotFound>
            <KError Code="404" />
        </NotFound>
    </Router>
    <KDialogContainer />
</CascadingValue>