<Tabs Animated>
    @if (Model.Items != null && Model.Items.Count > 0)
    {
        var key = 1;
        foreach (var item in Model.Items)
        {
            <TabPane Key="@($"{key++}")" Tab="@item.Title">
                @item.Content
            </TabPane>
        }
    }
</Tabs>

@code {
    [Parameter] public TabModel Model { get; set; }
}